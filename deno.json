{
  "tasks": {
    "dev": "deno run -A --node-modules-dir=auto npm:vite",
    "build": "deno run -A --node-modules-dir=auto npm:vite build",
    "server:start": "deno run -A --node-modules-dir --watch ./server/main.ts",
    "serve": "deno task build && deno task server:start"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@oak/oak": "jsr:@oak/oak@^17.1.3",
    "@std/assert": "jsr:@std/assert@1",
    "@tailwindcss/forms": "npm:@tailwindcss/forms@^0.5.10",
    "@tajpouria/cors": "jsr:@tajpouria/cors@^1.2.1",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.64.1",
    "@types/react": "npm:@types/react@^18.3.12",
    "@uidotdev/usehooks": "npm:@uidotdev/usehooks@^2.4.1",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.3",
    "autoprefixer": "npm:autoprefixer@^10.4.20",
    "formik": "npm:formik@^2.4.6",
    "postcss": "npm:postcss@^8.5.1",
    "prettier": "npm:prettier@^3.4.2",
    "prettier-plugin-tailwindcss": "npm:prettier-plugin-tailwindcss@^0.6.10",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-icons": "npm:react-icons@^5.4.0",
    "react-router-dom": "npm:react-router-dom@^7.1.1",
    "tailwindcss": "npm:tailwindcss@^3.4.17",
    "vite": "npm:vite@^5.4.11"
  },
  "compilerOptions": {
    "types": ["react", "react-dom", "@types/react"],
    "lib": ["dom", "dom.iterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
